            Categories(name: 'Vegetables', icon: 'assets/images/fruitsandwich.jpg'),
            Categories(name: 'Foods', icon: 'assets/images/fruitsandwich.jpg'),
            Categories(name: 'Drinks', icon: 'assets/images/fruitsandwich.jpg'),
            Categories(name: 'Fruits', icon: 'assets/images/fruitsandwich.jpg'),
